  langid = {english},
  file = {C\:\\Users\\Daniel\\Zotero\\storage\\5MDX798T\\Nosek et al_2022_Replicability, Robustness, and Reproducibility in Psychological Science.pdf;C\:\\Users\\Daniel\\Zotero\\storage\\8XTQAFU3\\annurev-psych-020821-114157.html}
